begin
//  dpAtalhosDir.AutoHide := False;
  dpChat.AutoHide := False;
           
  if not Permitido(daAtenderClientes) then 
//    dpAtalhosDir.Close 
  else begin
//    dpAtalhosDir.Visible := True;
{    nbgChat.Visible := True;
    nbgChat.Expanded := True;}
    dpChat.Visible := True;
  end;
//  Timer2.Enabled := True;
end;

